     "output_type": "execute_result"
    }
   ],
   "source": [
    "x = tf.constant(4.0)\n",
    "with tf.GradientTape() as tape:\n",
    "    i = 2.0 * x\n",
    "    tape.watch(x)\n",
    "    tape.watch(i)\n",
    "    output = f(i)\n",
    "grad = tape.gradient(output, i)\n",
    "grad.numpy()"
   ]
  }
